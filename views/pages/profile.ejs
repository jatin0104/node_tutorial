<%- include('../template/header'); -%>

<ul class="list-group">
    <% user.forEach(function(users,index) { %>
        <li class="list-group-item">Name: <%= users.firstName +' '+ users.lastName %> </li>
        <li class="list-group-item">Gender: <%= users.gender %> </li>
        <li class="list-group-item">Age: <%= users.age %></li>
        <li class="list-group-item">Number: <%= users.number %> </li>
    <% }) %>
</ul>

<%- include('../template/footer'); -%>